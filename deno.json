{
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 100,
        "exclude": [
            ".vscode"
        ]
    },
    "lint": {
        "rules": {
            "exclude": ["no-explicit-any"]
        }
    },
    "imports": {
        "@octokit/request": "npm:@octokit/request@^10.0.3",
        "@octokit/request-error": "npm:@octokit/request-error@^7.0.0",
        "@octokit/webhooks-types": "npm:@octokit/webhooks-types@^7.6.1",
        "@sentry/deno": "npm:@sentry/deno@^10.12.0"
    },
    "unstable": [
        "kv",
        "cron"
    ],
    "compilerOptions": {
        "noUncheckedIndexedAccess": true,
        "strictFunctionTypes": true
    }
}
