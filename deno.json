{
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 100,
        "exclude": [
            ".vscode"
        ]
    },
    "lint": {
        "rules": {
            "exclude": ["no-explicit-any"]
        }
    },
    "imports": {
        "@octokit/request": "npm:@octokit/request@^10.0.3",
        "@octokit/webhooks-types": "npm:@octokit/webhooks-types@^7.6.1"
    },
    "unstable": [
        "kv"
    ],
    "compilerOptions": {
        "noUncheckedIndexedAccess": true,
        "strictFunctionTypes": true
    }
}
